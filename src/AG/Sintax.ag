    | EnumBody enumConstants :: EnumConstants
               enumBodyDeclarations :: EnumBodyDeclarations

deriving EnumBody : Show

data EnumConstants
    | EnumConstants enumConstant :: EnumConstant enumConstants :: EnumConstants
    | NilEnumConstants

deriving EnumConstants : Show

data EnumConstant
    | EnumConstantAll annotations :: Annotations
                      identifier :: Identifier
                      argumentList :: ArgumentList
                      classBodyDeclarations :: ClassBodyDeclarations  {- Arguments por ArgumentList y () -}
    | EnumConstantArgL annotations :: Annotations
                       identifier :: Identifier
                       argumentList :: ArgumentList
    | EnumConstantClasB annotations :: Annotations
                        identifier :: Identifier
                        classBodyDeclaratiosn :: ClassBodyDeclarations
    | EnumConstantNothing annotations :: Annotations
                          identifier :: Identifier

deriving EnumConstant : Show

data EnumBodyDeclarations
    | EnumBodyDeclarations classBodyDeclarations :: ClassBodyDeclarations
    | NilEnumBodyDeclarations

deriving EnumBodyDeclarations : Show

data StaticInitializer
    | StaticInitializer blockStatements :: BlockStatements

deriving StaticInitializer : Show

data ConstructorBody
    | ConstructorBody explicitConstructorInvocation :: ExplicitConstructorInvocation
                                       blockStatements :: BlockStatements

deriving ConstructorBody : Show

data ExplicitConstructorInvocation
    | ExplConsInvThis nonWildTypeArguments :: NonWildTypeArguments
                      argumentList :: ArgumentList
    | ExplConsInvSuper nonWildTypeArguments :: NonWildTypeArguments
                       argumentList :: ArgumentList
    | ExplConsInvPrimary primary :: Primary
                         argumentList :: ArgumentList {- NonWildTypeArguments ArgumentList -}
    | NilExplixitConsInv

deriving ExplicitConstructorInvocation : Show

