data ConditionalOrExpression
    | Mod -- :%:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | Or -- :||:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | And -- :&&:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | BitwiseOr  -- :|:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | BitwiseXor -- :^:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | BitwiseAnd -- :&:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | EqualTo -- :==:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | NotEqualTo -- :!=:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | LessThan -- :<:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | GreaterThan -- :>:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | LessThanOrEqualTo -- :<=:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | GreaterThanOrEqualTo -- :>=:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | LeftShift -- :<<:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | RightShift -- :>>:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | ZeroFillRightShift -- :>>>:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | Add -- :+:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | Sub -- :-:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | Mult -- :*:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | Div -- :/:
        conditionalOrExpression1 :: ConditionalOrExpression
        conditionalOrExpression2 :: ConditionalOrExpression
    | ConditionalOrExpressionUnaryExpression unaryExpression :: UnaryExpression
    | ConditionalOrExpressionIntanceOf unaryExpression :: UnaryExpression
                                     type_ :: Type
deriving ConditionalOrExpression : Show

