type Catches = [CatchClause]

data CatchClause
    | CatchClause variableModifiers :: VariableModifiers
                  type_ :: Type
                  variableDeclaratorId :: VariableDeclaratorId
                  blockStatements :: BlockStatements

deriving CatchClause : Show
