-- | Defines the data types
module {AG.Sintax}
{} -- Exports of this module, empty for none
{
-- Imports
}


data Tree
   | Node  left  :: Tree
           right :: Tree
   | Tip   value :: Int

attr Tree
   syn sum :: Int

sem Tree
  | Node  lhs.sum  =  @left.sum + @right.sum
  | Tip   lhs.sum  =  @value
